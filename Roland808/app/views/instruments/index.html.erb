<% @instruments.each do |instrument| %>
<br>
<%= link_to(instrument.name, instrument) %>


 <ul id="<%=instrument.id%>" class="nav">
       <li>
        <a href="#">Home</a>
        <audio id="beep-one" preload="auto">
        <source src="<%= instrument.sound %>"></source>
        <source src="audio/beep.ogg"></source>
        Your browser isn't invited for super fun time.
      </audio>
      </li>
    </ul>

    <script>
    var beepOne = $("#<%= instrument.id %>")[0];
    $("#<%=instrument.id%>")
    .mouseenter(function() {
    beepOne.play();
    });
    </script>

<br>
<%= link_to 'Delete', instrument, :method => :delete, :confirm => 'Okay to delete?' %>
<br>
<%= link_to 'Edit', edit_instrument_path(instrument) %>
<br>
<% end %>





