<ul id="nav-three" id="soundid">
<p class="blip">
<%= image_tag(@instrument.pic) %>
</p>
        <audio id="beep-three" preload="auto">
        <source src='<%= @instrument.sound %>'></source>
        Your browser isn't invited for super fun time.
      </audio>
    <script>$("p.blip")
  .each(function(i) {
    if (i >= 0) {
      $("#beep-three")
        .attr("id", "beep-three" + i)
        .appendTo($(this).parent());
    }
    $(this).data("beeper", i);
  })
  .mouseenter(function() {
    $("#beep-three" + $(this).data("beeper"))[0].play();
  });
$("#beep-three").attr("id", "beep-three");</script>
<%= link_to 'Delete', @instrument, :method => :delete, :confirm => 'Okay to delete?' %>
<%= link_to 'Edit', edit_instrument_path(@instrument) %>
</ul>
