<%= form_for @artist do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :dob %>
<%= f.text_field :dob %>

<%= f.label :nationality %>
<%= f.text_field :nationality %>

<%= f.label :image %>
<%= f.text_field :image %>

<%= f.label :period %>
<%= f.text_field :period %>

<%= f.submit %>

<% end %>