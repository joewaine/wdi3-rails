<%= form_for @painting do |f| %>

<%= f.label :artist_id %>
<%= f.select :artist_id, Artist.all.map{|artist| [artist.name, artist.id]} %>
<br>
<%= f.label :title %>
<%= f.text_field :title %>
<br>
<%= f.label :image %>
<%= f.text_field :image %>
<br>
<%= f.label :style %>
<%= f.text_field :style %>
<br>
<%= f.label :year %>
<%= f.text_field :year %>
<br>
<%= f.label :period %>
<%= f.text_field :period %>
<br>
<%= f.submit %>

<% end %>